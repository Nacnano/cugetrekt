generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  email       String
  password    String
  title       Int?
  name        String?
  surname     String?
  studentId   String?   @unique
  faculty     String?
  department  String?
  studySystem Int?
  tel         String?
  infoEmail   String?
}

model Resignation {
  id          Int     @id @default(autoincrement())
  userId      Int
  docsName    String?
  lastEdit    String?
  title       Int?
  name        String?
  surname     String?
  studentId   String?
  faculty     String?
  department  String?
  tel         String?
  email       String?
  semester    Int?
  year        String?
  studySystem Int?
  reason      String?
}

model Withdrawal {
  id          Int     @id @default(autoincrement())
  userId      Int
  docsName    String?
  lastEdit    String?
  title       Int?
  name        String?
  surname     String?
  studentId   String?
  faculty     String?
  department  String?
  tel         String?
  email       String?
  gpax        String?
  status      Int?
  credit      Float?
  semester    Int?
  year        String?
  studySystem Int?
  course1     String?
  course2     String?
  course3     String?
  reason1     String?
  reason2     String?
  reason3     String?
}

model DocumentType {
  id          Int       @id @default(autoincrement())
  docName     String
}

model Course {
  id          Int       @id @default(autoincrement())
  courseId    String    @unique
  courseName  String
  courseAbbr  String
}