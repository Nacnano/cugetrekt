generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  studentId   String   @unique
  password    String
}

model Course {
  id          Int       @id @default(autoincrement())
  courseId    String    @unique
  courseName  String
  courseAbbr  String
  mean        Float
  sd          Float
}